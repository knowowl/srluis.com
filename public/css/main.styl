rojo1 = #9A1A27
rojo2 = #741620
rojo3 = #4D0910

body
	background white url("http://www.know-owl.com/img/srluis/background_image.jpg") 50% 0% fixed no-repeat
.brand
	background transparent url("http://know-owl.com/img/srluis/isologo_small3.png") no-repeat
	width 124px
	height 36px
	padding 0 !important
	margin 5px 0 0px 0px !important
	float left 
	position relative	
	z-index: 2
.container
	width 100% !important
.navbar-inner
	background rojo1 url("http://know-owl.com/img/srluis/navbar-bg2.jpg") !important
	border 0 !important
	height 35px !important
	border-radius 0 !important
	box-shadow 0px 1px 0px rojo3 !important
.navbar
	margin-bottom 0px
	height 35px !important	
.search
	background transparent !important
	border 0 !important
	padding 10px 0 3px 0px !important
	margin 0 0 0 0px !important
	height 25px !important
	color white !important
	font-family 'Muli', sans-serif
	font-size 1.2em !important
	box-shadow none !important
	font-weight 400
	text-shadow 0 -1px 0 rojo2
.searchList
	position relative
	float left
	height 0px
	background white
	margin 0
	width 100%
	opacity .9
	border-bottom:1px solid #666;
.category
	padding 0
	margin 0
	float left
.category>li
	padding 10px 5px 0px 5px
	float left
	width 80px 
	height: 80px
	background transparent
	list-style none
	display none
.category li img
	border-radius 6px
	margin0 5px 0px 5px !important
	float left
.category li label
	float left
	width 80px
	text-align center
.category a
	color #999
	float left
.category a:hover
	color #333
form 
	margin 0 !important
	float left
.search::-webkit-input-placeholder 
	color white
.search:-moz-placeholder 
	color white
.search::-moz-placeholder 
	color white
.search:-ms-input-placeholder 
	color white 
.searchIcon
	float left
	color rojo1
	margin 10px 5px 0px -15px
	padding 3px  0px 0 3px 
	width 28px
	height 25px
	text-align left
	text-shadow 0 1px 0 rojo3
	background transparent url("http://know-owl.com/img/srluis/search_bg.png") no-repeat !important
.settings
	padding 0
	float left
	color rojo2
	padding 5px 0px 0 5px
	text-align center
	text-shadow 0 -1px 0 rojo3
	font-size 1.3em
.dropdown-toggle
	outline 0 !important
	background transparent !important
.dropdown-menu
	margin-left 12px
.carousel	
	float left !important
#results
	float:left;
.product-box
	display none
	box-shadow rgba(0,0,0,.2) 0 0px 3px
	float left
	border-radius 5px
	width 240px
	margin 10px	
	cursor hand
	cursor pointer
.product
	box-shadow 0 8px 3px -5px rgba(0,0,0,.2)
	float left
	border-radius 100% / 33px	
.product-inner
	float left
	border-radius 5px
	background -webkit-linear-gradient(top, rgba(249,249,249,1) 70%, rgba(224, 220, 220, 1) 90%)
.product-inner>.Peek
	font-size 0.85em
	font-weight 500	
	color #888
	margin 5px
.product-inner>.Name, .product-inner>.Store
	margin 5px 5px 0 5px
	font-size 1.2em
	font-weight 500
	text-shadow rgba(0,0,0,.6) 0 0px 1px
	color #222	
.product-inner>img
	margin 5px
	width 230px
.results
	list-style none
.product-inner>.Price
	width 90px
	float right
	text-align right	
	border-bottom-right-radius 5px
	background rgb(235, 235, 235)
	padding 5px 5px 5px 0
	border-top 1px solid 1px solid rgb(201, 199, 199)
.product-inner>.Likes
	width 140px
	float left
	text-align left
	border-bottom-left-radius 5px	
	background rgb(235, 235, 235)
	padding 5px 0 5px 5px
	border-top 1px solid 1px solid rgb(201, 199, 199)
.fb-like span,.fb-like, iframe.fb_ltr 
	position relative
	float left
	min-width 140px
	min-height 20px 
#cartList
	float left
	position fixed
	right 0
	top 38px
	width 200px
	height 100%
	background white
	border-left 1px solid #999
	box-shadow 0 0 3px rgba(0,0,0,.3)
	overflow auto
.cart
	position fixed
	top 0
	right 0
	width 200px
	height 41px
	border 1px solid rgb(172, 67, 67)
	border-bottom-left-radius 5px
	z-index 100
	border-top 0
	border-right 0
	box-shadow -1px 1px 2px rgba(0,0,0,.4)
	background rojo1 url("http://know-owl.com/img/srluis/navbar-bg2.jpg") !important