terracota = #D34836
terracotaClaro = #F0D7BA

.brand
	background  terracota url("http://know-owl.com/img/srluis/isologo_small.jpg") no-repeat
	width 135px
	height 25px
	padding 0 !important
	margin 6px 0 5px 10px
.navbar-inner
	background terracota !important
	border 0 !important
.navbar
	box-shadow 0px 1px 1px #5F160D
.search
	background transparent !important
	border 0 !important
